{
    "build": {
        "name": "libxutils.a",
        "outputDir": "./build",
        "flags": "-g -O2 -Wall -D_XUTILS_DEBUG -D_XUTILS_USE_GNU -D_ASSERT_TIMED",
        "libs": "-lpthread",
        "overwrite": true,
        "verbose": 2,

        _SSL_OBJECT_

        "sources": [
            _SOURCE_LIST_
        ],

        "includes": [
            "./src/crypt",
            "./src/data",
            "./src/net",
            "./src/sys",
            "./src"
        ]
    },

    "install": {
        "headerDir": "_INSTALL_PREFIX_/include/xutils",
        "binaryDir": "_INSTALL_PREFIX_/lib"
    }
}
